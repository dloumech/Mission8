@{
    ViewData["Title"] = "Time-Management Matrix";
}


<div class="container">
    <div class="row">
        <div class="col">
            <h2>Stephen Covey's Time Management Matrix'</h2>
        </div>
    </div>

    <div class="row">
        <div class="col-2" style="background-color:black"></div>
        <div class="col-5">
            <h3>Urgent</h3>
        </div>
        <div class="col-5">
            <h3>Not Urgent</h3>
        </div>
    </div>

    <div class="row">
        <div class="col-2" style="background-color:black">
            Important
        </div>
        <div class="col-5">
            <h3>Quadrant 1</h3>
        </div>
        <div class="col-5">
            <h3>Quadrant 2</h3>
        </div>
    </div>

    <div class="row">
        <div class="col-2" style="background-color:black">
            Not Important
        </div>
        <div class="col-5">
            <h3>Quadrant 3</h3>
        </div>
        <div class="col-5">
            <h3>Quadrant 4</h3>
        </div>
    </div>
</div>